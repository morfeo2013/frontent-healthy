\n  return (\r\n    <div>\r\n      <div className=\"row pt-4\">\r\n        <div className=\"col-lg-6 col-md-8 mx-auto\">\r\n          <div className=\"card\">\r\n            <div className=\"container text-center fa-4x\">\r\n              <i className=\"fas fa-user\"></i>\r\n            </div>\r\n            <div className=\"card-header text-center\">Recuperar Contraseña</div>\r\n\r\n            <div className=\"card-boddy m-2\">\r\n              <form onSubmit={login}>\r\n                <div className=\"form-group mt-2\">\r\n                  <input\r\n                    type=\"email\"\r\n                    name=\"correo\"\r\n                    className=\"form-control\"\r\n                    /* autofocus inicia el cursor en este lugar del form */\r\n                    placeholder=\"Ingresa tu Correo\"\r\n                    required\r\n                    autoFocus\r\n                    onChange={(e) => setCorreo(e.target.value)}\r\n                    value={correo}\r\n                  />\r\n                </div>\r\n\r\n                <div className=\"d-grid gap-2 m-2\">\r\n                  <button\r\n                    type=\"submit\"\r\n                    className=\" btn btn-primary mt-3 btn-block\"\r\n                  >\r\n                    Aceptar\r\n                  </button>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\NEON\\Documents\\SERVIDOR FULL\\FRONTEND\\src\\components\\PiedePagina.js",[],"C:\\Users\\NEON\\Documents\\SERVIDOR FULL\\FRONTEND\\src\\components\\Nav.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":2,"column":8,"nodeType":"79","messageId":"80","endLine":2,"endColumn":12},{"ruleId":"77","severity":1,"message":"81","line":4,"column":10,"nodeType":"79","messageId":"80","endLine":4,"endColumn":14},"no-native-reassign",["82"],"